    "version": "0.2.0",
    "configurations": [

        {
            "name": "DisplayImage",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/DisplayImage",
            "args": ["${workspaceFolder}/images/img1.png"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
